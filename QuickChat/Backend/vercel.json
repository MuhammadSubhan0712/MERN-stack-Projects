{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "OPTIONS"]
    }
  ]
}
